<!-- ui_game_symbol_backpack -->

<configs>
		
	<append xpath="/nav_object_classes">
		
		<nav_object_class name="navBloodmoonHordeReward">
			<map_settings>
				<property name="sprite_name" value="ui_game_symbol_twitch_drop"/>
				<property name="min_distance" value="0"/>
				<property name="max_distance" value="100"/>
				<property name="color" value="192,53,41,255"/>
				<property name="has_pulse" value="true"/>
			</map_settings>

			<compass_settings>
				<property name="sprite_name" value="ui_game_symbol_twitch_drop"/>
				<property name="min_distance" value="0"/>
				<property name="max_distance" value="100"/>
				<property name="max_scale_distance" value="1024"/>
				<property name="color" value="192,53,41,255"/>
				<property name="has_pulse" value="true"/>
				<property name="icon_clamped" value="false" />
			</compass_settings>

			<onscreen_settings>
				<property name="sprite_name" value="ui_game_symbol_twitch_drop"/>
				<property name="min_distance" value="0"/>
				<property name="max_distance" value="100"/>
				<property name="color" value="192,53,41,255"/>
				<property name="has_pulse" value="true"/>
				<property name="text_type" value="Distance"/>
				<property name="offset" value="0,1,0" />
			</onscreen_settings>
		</nav_object_class>
		
	</append>
</configs>
