			<property name="Extends" value="paintingAvengers"/>
			<property name="CustomIcon" value="paintingShinobiRevenge" />
			<!--<property name="DescriptionKey" value="paintingShinobiRevengeDesc" />-->
			<property name="Model" value="#@modfolder:Resources/Pictures_MD.unity3d?PictureShinobiRevengePrefab"/>
			<property name="SortOrder2" value="0007"/>
