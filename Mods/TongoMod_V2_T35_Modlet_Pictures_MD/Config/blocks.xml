<?xml version="1.0" encoding="UTF-8"?>
<raiz>

	<append xpath="/blocks">

		<!-- *** paintings -->

		<block name="paintingAvengers">
			<property name="CreativeMode" value="Player"/>
			<property name="Material" value="Mglass"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="CustomIcon" value="paintingAvengers" />
			<!--<property name="DescriptionKey" value="paintingAvengersDesc" />-->
			<property name="Model" value="#@modfolder:Resources/Pictures_MD.unity3d?PictureAvengersPrefab" param1="GamePicturesVariantHelper"/>/>
			<property name="ImposterDontBlock" value="true"/>
			<property name="WaterFlow" value="permitted"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="IsDecoration" value="true"/>
			<!--<property name="CanPickup" value="true"/>-->
			<property name="Collide" value="sight,movement,melee,bullet,arrow,rocket" />
			<property name="StabilitySupport" value="false"/>
			<drop event="Destroy" name="resourceWood" count="1,2"/>
			<property name="FilterTags" value="MC_building,SC_decor"/>
			<property name="SortOrder1" value="B259"/>
			<property name="SortOrder2" value="0001"/>
		</block>

		<block name="paintingCastlevania">
			<property name="Extends" value="paintingAvengers"/>
			<property name="CustomIcon" value="paintingCastlevania" />
			<!--<property name="DescriptionKey" value="paintingCastlevaniaDesc" />-->
			<property name="Model" value="#@modfolder:Resources/Pictures_MD.unity3d?PictureCastlevaniaPrefab"/>
			<property name="SortOrder2" value="0002"/>
		</block>

