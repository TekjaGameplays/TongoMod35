		<block name="GamePicturesVariantHelper">
			<property name="Extends" value="paintingAvengers" param1="Place"/>
			<property name="CreativeMode" value="Player"/>
			<property name="CustomIcon" value="paintingSonic"/>
			<property name="ItemTypeIcon" value="all_blocks"/>
			<property name="DescriptionKey" value="GamePicturesVariantHelperDesc"/>
			<property name="SelectAlternates" value="true"/>
			<property name="PlaceAltBlockValue" value="paintingAvengers,paintingCastlevania,paintingGoldenAxe,paintingMoonwalker,paintingPs4,paintingSf2,paintingShinobiRevenge,paintingSonic,paintingXmen,paintingGng"/>
			<property name="SortOrder1" value="V100"/>
			<property name="SortOrder2" value="0000"/>
			<property name="FilterTags" value="MC_playerBlocks,SC_playerHelpers"/>
		</block>


